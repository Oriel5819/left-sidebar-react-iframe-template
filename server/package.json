    "helmet": "^5.0.2",
    "mongoose": "^6.2.8",
    "morgan": "^1.10.0",
    "react-thunk": "^1.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
