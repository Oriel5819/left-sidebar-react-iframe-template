{
  "name": "template-leftmenu",
  "version": "1.0.0",
  "description": "To make it easy for you to get started with GitLab, here's a list of recommended next steps.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/app.js",
    "server": "nodemon src/app.ts",
    "client": "cd ../client && npm start",
    "colors": "cd ../colors && npm start",
    "collaborator": "cd ../collaborator && npm start",
    "all": "concurrently \"npm run server\" \"npm run client\" \"npm run colors\" \"npm run collaborator\" \"npm run main\" ",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
